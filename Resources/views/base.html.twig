<!-- Body -->
<body {% block body_tag '' %} >
    {% block body '' %}
    {% block body_post '' %}
    {% block core_javascripts %}
        {% javascripts 
            'bundles/tecnocreacionescrmtemplate/template/html/assets/js/bootstrap.min.js' 
            'bundles/tecnocreacionescrmtemplate/template/html/assets/js/beyond.min.js' 
            'bundles/tecnocreacionescrmtemplate/template/html/assets/js/toastr/toastr.js' 

            filter='uglifyjs2' 
            output="assets/template_base.min.js"
        %}
            <script src="{{ asset_url }}"></script>
        {% endjavascripts %}
    {% endblock core_javascripts %}
    
    {% block javascripts '' %}
        
    <script type="text/javascript">
        var flashManager = {
            time: "10000",
            danger: function(message,parameters){
                this.show(message,'danger', 'fa-times');
            },
            success: function(message,parameters){
                this.show(message,'success', 'fa-check');
            },
            warning: function(message,parameters){
                this.show(message,'warning', 'fa-warning');
            },
            info: function(message,parameters){
                this.show(message,'info', 'fa-info');
            },
            debug: function(message,parameters){
                {% if app.debug %}
                    console.log(message);
                    this.show("Debug: "+message,'maroon', 'fa-home');
                {% endif %}
            },
            show: function(message,type,icon){
                if(message != ""){
                    Notify(message, 'top-right', this.time, type, icon, true);
                }
                {% if app.debug %}
                    console.log("debug show: "+message+" type: "+type);
                {% endif %}
            }
        };
        $(document).ready(function(){
            {% for type, messages in flashes %}
{#                flashManager.debug("Tipo - {{ type }}");#}
                {% for message in messages %}
                    
                    {% if type == "success" %}
                        flashManager.success("{{ message }}");
                    {% elseif type == "error" %}
                        flashManager.danger("{{ message }}");
                    {% elseif type == "warning" %}
                        flashManager.warning("{{ message }}");
                    {% elseif type == "info" %}
                        flashManager.info("{{ message }}");
                    {% elseif type == "debug" %}
                        flashManager.debug("{{ message }}");
                    {% endif%}
                {% endfor %}
            {% endfor %}
        });
    </script>
    {% block javascript '' %}
    {% if app.debug == false %}
        {% block analytics '' %}
    {% endif%}
</body>
<!--  /Body -->
</html>
